        <h1>Client demo socket.io</h1><hr>

        <!-- script xu ly -->
        <script>
            var socket = io("http://localhost:3000");
            $(document).ready(function(){
                socket.emit("listen-client", "Du lieu gui len");

                socket.on("server-chao", function(data){
                    alert(data);
                });
            });
        </script>

    </body>
